<form action="<%= action %>" method="post">
  <%= auth_token %>
  <label>
    Username <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>
  <label>
    Password <input type="password" name="user[password]" value="">
  <label>

  <input type="submit" value="<%= submit_text %>">
</form>
